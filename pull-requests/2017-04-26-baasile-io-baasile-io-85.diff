+module Bills
+  class GeneratePdfBillService
+    REFERENCE_WIDTH = 290
+    REFERENCE_UNIT_PRICE_WIDTH = 50
+    REFERENCE_UNIT_WIDTH = 50
+    REFERENCE_TOTAL_PRICE_WIDTH = 50
+    REFERENCE_VAT_RATE_WIDTH = 50
